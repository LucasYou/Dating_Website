<!doctype html>
<html lang="en">

<head>
    {{> head_element }}
</head>

<body>
    {{> header }}
	<div class="container" style="margin-top : 50px">
        <h1 style="margin: 0px 0px 30px 20px;">All Revenue by Customer Representative</h1>

		<table id="example" class="table table-striped table-bordered" style="width:100%">
		        <thead>
		            <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>SSN</th>
                        <th>Total Revenue</th>

		            </tr>
		        </thead>
		        <tbody>
                    {{#each revenue_custreps}}
                        <tr>
                            <td>{{firstname}}</td>
                            <td>{{lastname}}</td>
                            <td>{{customer_rep}}</td>
                            <td>{{revenue}}</td>
                        </tr>
                    {{/each}}
		        </tbody>
		        <tfoot>
		            <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>SSN</th>
                        <th>Total Revenue</th>
		            </tr>
		        </tfoot>
		    </table>
	</div>

    {{> table_script }}
  </body>
</html>
