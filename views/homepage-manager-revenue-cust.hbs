<!doctype html>
<html lang="en">

<head>
    {{> head_element }}
</head>

<body>
    {{> header }}
    <div class="container" style="margin-top : 50px">
        <h1 style="margin: 0px 0px 30px 20px;">All Revenue by Customer</h1>

        <table id="example" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>User</th>
                    <th>Total Revenue</th>

                </tr>
            </thead>
            <tbody>
                {{#each revenue_custs}}
                <tr>
                    <td>{{customer}}</td>
                    <td>{{revenue}}</td>
                </tr>
                {{/each}}
            </tbody>
            <tfoot>
                <tr>
                    <th>User</th>
                    <th>Total Revenue</th>
                </tr>
            </tfoot>
        </table>
    </div>

    {{> table_script }}
</body>

</html>